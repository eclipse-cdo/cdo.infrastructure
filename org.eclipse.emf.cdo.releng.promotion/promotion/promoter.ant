<?xml version="1.0"?>
<project name="promoter" default="promoter">

	<target name="promoter">
		<java classname="org.eclipse.emf.cdo.releng.promotion.Promoter" dir="${promotionWorkDir}" fork="true">
			<classpath path="classes" />
			<arg value="${downloadsDir}" />
			<arg value="${hudsonJobsDir}" />
			<arg value="${jobName}" />
			<arg value="${lastBuildNumber}" />
			<arg value="${nextBuildNumber}" />
		</java>
	</target>

</project>
