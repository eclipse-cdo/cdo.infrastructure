<?xml version="1.0"?>
<project name="classes" default="copy-classes">

	<target name="copy-classes">
		<delete includeemptydirs="false" failonerror="false">
			<fileset dir="promotion/classes">
				<include name="**" />
			</fileset>
		</delete>

		<copy todir="promotion/classes">
			<fileset dir="bin">
				<include name="**" />
			</fileset>
		</copy>
	</target>

</project>
